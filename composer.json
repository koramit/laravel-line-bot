        "illuminate/support": "^11.43",
        "illuminate/database": "^11.43"
    },
    "require-dev": {
        "laravel/pint": "^1.21"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Koramit\\LaravelLINEBot\\": "src/"
        }
    },
    "authors": [
        {
